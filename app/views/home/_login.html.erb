<h2>Login</h2>
<%= form_for(User.new, url: user_session_path) do |f| %>
  <%= f.email_field :email, placeholder: "Email", autofocus: true %>
  <%= f.password_field :password, placeholder: "Password" %>
  <%= f.check_box :remember_me %>
  <%= f.label :remember_me, "Remember me" %>
  <%= f.submit "Login" %>
<% end %>
